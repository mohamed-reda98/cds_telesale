<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="sale_order_print_report" model="ir.actions.report">
        <field name="name">Sale Order Lines</field>
        <field name="model">sale.order</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">cds_telesale.report_orderline</field>
        <field name="report_file">cds_telesale.report_orderline</field>
        <field name="binding_model_id" ref="model_sale_order"/>
        <field name="binding_type">report</field>
    </record>

    <template id="report_orderline">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="web.external_layout">
                    <div class="page">
                    </div>
                </t>
            </t>
        </t>
    </template>

</odoo>